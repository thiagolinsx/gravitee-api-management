{
  "id": "my-api",
  "name": "my-api",
  "gravitee": "2.0.0",
  "proxy": {
    "context_path": "/test",
    "endpoints": [
      {
        "name": "default",
        "target": "http://localhost:8080/team",
        "http": {
          "connectTimeout": 3000,
          "readTimeout": 60000
        }
      }
    ]
  },
  "flows": [
    {
      "name": "flow-1",
      "methods": [
        "GET"
      ],
      "enabled": true,
      "path-operator": {
        "path": "/",
        "operator": "STARTS_WITH"
      },
      "pre": [
        {
          "name": "Transform Headers",
          "description": "",
          "enabled": true,
          "policy": "transform-headers",
          "configuration": {
            "addHeaders": [
              {
                "name": "headerKey",
                "value": "headerValue"
              }
            ],
            "scope": "REQUEST"
          }
        }
      ],
      "post": [
        {
          "name": "Assign content",
          "description": "",
          "enabled": true,
          "policy": "assign-content",
          "configuration": {
            "body": "test-content",
            "scope": "RESPONSE"
          }
        }
      ]
    }
  ]
}
